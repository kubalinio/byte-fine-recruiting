{
  "name": "byte-fine-recruitment",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:analyze": "cross-env ANALYZE=1 npm run build",
    "build:ci": "cross-env VITE_CI=1 vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" && stylelint \"./src/**/*.{css,pcss,scss}\" && echo \"lint success\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\" && stylelint --fix \"./src/**/*.{css,pcss,scss}\" && echo \"lint success\"",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest",
    "translations": "ts-node -T --project tsconfig.json ./scripts/fetch-translations.ts",
    "typecheck": "tsc --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,html,css}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.{css,pcss,scss}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "3.10.0",
    "@radix-ui/react-alert-dialog": "^1.1.5",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-query": "^5.63.0",
    "@tanstack/react-router": "^1.97.25",
    "@tanstack/router-plugin": "^1.97.25",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.0",
    "cmdk": "^1.0.0",
    "downloadjs": "^1.4.7",
    "html-to-image": "^1.11.11",
    "lucide-react": "^0.469.0",
    "msw": "^2.7.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-hook-form": "^7.54.2",
    "react-intl": "^7.0.4",
    "react-moveable": "^0.56.0",
    "uuid": "^11.0.5",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.8.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.0",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tanstack/react-query-devtools": "^5.63.0",
    "@tanstack/router-devtools": "1.95.5",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "15.0.6",
    "@testing-library/user-event": "14.5.2",
    "@types/downloadjs": "^1.4.6",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@welldone-software/why-did-you-render": "8.0.1",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "globals": "^15.14.0",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "~22.1.0",
    "lint-staged": "15.3.0",
    "postcss": "8.4.38",
    "prettier": "3.4.2",
    "prettier-plugin-packagejson": "2.5.6",
    "prettier-plugin-tailwindcss": "0.6.9",
    "stylelint": "16.3.1",
    "stylelint-config-recommended": "14.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
